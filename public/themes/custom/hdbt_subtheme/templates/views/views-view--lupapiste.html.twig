{#
/**
 * @file
 * Theme override for main view template.
 */
#}
{%
  set classes = [
    'views',
    'views--' ~ id|clean_class,
    'views--' ~ display_id|clean_class,
    dom_id ? 'js-view-dom-id-' ~ dom_id,
  ]
%}
<div{{attributes.addClass(classes)}}>
  {{ title_prefix }}
  {{ title }}
  {{ title_suffix }}

  {% if header %}
    <header>
      {{ header }}
    </header>
  {% endif %}

  {{ drupal_block('lupapiste_rss_feed') }}

</div>
